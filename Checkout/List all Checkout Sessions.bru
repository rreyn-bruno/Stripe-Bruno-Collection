meta {
  name: List all Checkout Sessions
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/v1/checkout/sessions
  body: none
  auth: bearer
}

params:query {
  ~created[gt]: <integer>
  ~created[gte]: <integer>
  ~created[lt]: <integer>
  ~created[lte]: <integer>
  ~customer: <string>
  ~customer_details[email]: <string>
  ~ending_before: <string>
  ~expand[0]: <string>
  ~expand[1]: <string>
  ~limit: <integer>
  ~payment_intent: <string>
  ~payment_link: <string>
  ~starting_after: <string>
  ~status: <string>
  ~subscription: <string>
}

headers {
  Content-Type: application/x-www-form-urlencoded
}

auth:bearer {
  token: <Bearer Token>
}

docs {
  <p>Returns a list of Checkout Sessions.</p>
}
